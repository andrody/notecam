<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <!--stroke android:width="1dip" android:color="#b7b9ac" /-->
            <gradient  android:angle="-90"  android:startColor="#528a98" android:endColor="#72adbb"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <!--stroke android:width="1dip" android:color="#b7b9ac" /-->
            <solid  android:color="#82c8d9"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <!--stroke android:width="1dip" android:color="#b7b9ac" /-->
            <gradient  android:angle="-90"  android:startColor="#82c8d9" android:endColor="#82c8d9" />
        </shape>
    </item>
</selector>